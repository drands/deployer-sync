{
    "name": "drands/deployer-sync",
    "description": "A Laravel package to synchronize database and uploaded files between local and remote servers using Deployer.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drands\\DeployerSync\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drands\\DeployerSync\\DeployerSyncServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.5"
    }
}